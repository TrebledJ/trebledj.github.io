    "license": "LGPL-3.0",
    "devDependencies": {
        "@testdeck/mocha": "^0.3.3",
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.1.5",
        "@typescript-eslint/eslint-plugin": "^5.59.6",
        "@typescript-eslint/parser": "^5.59.6",
        "chai": "^4.3.7",
        "eslint": "^8.40.0",
        "eslint-plugin-jsdoc": "^44.2.4",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-react": "^7.32.2",
        "mocha": "^10.2.0",
        "prettier": "^2.8.8",
        "source-map-support": "^0.5.21",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "franc": "^6.1.0",
